# Server Configuration
PORT=3001
NODE_ENV=development

# Firebase Configuration
# Note: API credentials are stored securely in Firestore

# Ethereum Provider (for Uniswap integration)
ETH_PROVIDER_URL=https://mainnet.infura.io/v3/your_infura_api_key
INFURA_API_KEY=your_infura_api_key

# JWT Secret for token signing (if implementing user authentication)
JWT_SECRET=your_jwt_secret_key

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10

# Cache Configuration
CACHE_TTL_MS=60000

# Encryption Key for additional security (optional)
# ENCRYPTION_MASTER_KEY=your_encryption_master_key
