{
  "dashboard": {
    "id": null,
    "title": "Strategiz Application Overview",
    "tags": ["strategiz", "overview", "logs", "metrics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"strategiz-app\"}",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate (RPS)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"strategiz-app\"}[5m])",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"strategiz-app\",status=~\"5..\"}[5m]) / rate(http_server_requests_seconds_count{job=\"strategiz-app\"}[5m]) * 100",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Response Time (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"strategiz-app\"}[5m]))",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Recent Error Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"strategiz-app\"} |= \"ERROR\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Log Levels Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by(level) (rate({job=\"strategiz-app\"} [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "API Endpoints Performance",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, avg by (uri) (rate(http_server_requests_seconds_sum{job=\"strategiz-app\"}[5m]) / rate(http_server_requests_seconds_count{job=\"strategiz-app\"}[5m])))",
            "refId": "A",
            "format": "table",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Authentication Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"strategiz-app\"} |~ \"passkey|oauth|signup|signin\" | json",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "User Activity by Endpoint",
        "type": "piechart",
        "targets": [
          {
            "expr": "topk(8, sum by (uri) (rate(http_server_requests_seconds_count{job=\"strategiz-app\", uri!~\"/actuator.*\"}[5m])))",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "links": []
  }
} 